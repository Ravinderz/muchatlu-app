{
  "expo": {
    "name": "Muchatlu",
    "slug": "muchatlu-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification":{
      "icon":"./assets/notification_icon.png"
   },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "in.Muchatlu.muchatlu",
      "supportsTablet": true,
      "buildNumber": "1.0.1"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "in.Muchatlu.muchatlu",
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/notification_icon.png"
    }
  }
}
